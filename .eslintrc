{
  "parser": "babel-eslint",
  "extends": [
    "eslint-config-airbnb"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "react/jsx-indent": [
      2,
      4
    ],
    "react/jsx-indent-props": [
      2,
      4
    ],
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "semi": 0,
    "arrow-parens": 0,
    "linebreak-style": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "no-console": 2,
    "no-alert": 2,
    "jsx-a11y/href-no-hash": 0,
    "react/jsx-filename-extension": [
        "error",
        {
            "extensions": [
              ".js",
              ".jsx"
            ]
        }
    ],
    "react/no-array-index-key": "warn"
  },
  "globals": {
    "__PRO__": true,
    "YTNavigation": true,
    "YTPay": true,
    "YTNetwork": true,
    "YTLogicService": true,
    "YTNotification": true
  }
}
